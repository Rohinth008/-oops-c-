#include<iostream>

class NumberSwapper {
public:
    void swapNumbers() {
        int num1, num2;

        std::cout << "Enter two numbers: ";
        std::cin >> num1 >> num2;

        // Swap numbers using std::swap
        std::swap(num1, num2);

        std::cout << "After swapping: First number = " << num1 << ", Second number = " << num2 << std::endl;
    }
};

int main() {
    NumberSwapper swapper;
    swapper.swapNumbers();

    return 0;
}

