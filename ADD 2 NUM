#include<iostream>

class Calculator {
public:
    double add(double num1, double num2) {
        return num1 + num2;
    }
};
int main() {
    Calculator calculator;
    double num1, num2;
    std::cout << "Enter two numbers: ";
    std::cin >> num1 >> num2;
    std::cout << "Sum: " << calculator.add(num1, num2) << std::endl;
    return 0;
}
